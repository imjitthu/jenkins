pipeline {
   agent any
   stages {
       stage('Write File') {
           steps {
               script {
                   def date = new Date()
                   def data = sh "ls -alh | sort  > htop.txt"
                   writeFile(file: 'aws.txt', text: data)
                   sh "ls -l"
               }
           }
       }
       stage('Read File') {
           steps {
               script {
                   def data = readFile(file: 'htop.txt')
                   println(data)
               }
           }
       }
   }
}