pipeline {

  agent any

  options {
    copyArtifactPermission 'test-pipeline'
    durabilityHint 'MAX_SURVIVABILITY'
  }
  stages {

    stage('frontend') {
      steps {
        powershell 'dir'
        archiveArtifacts artifacts: '*', followSymlinks: false, onlyIfSuccessful: true
        powershell 'dir'
        sshPublisher(publishers: [sshPublisherDesc(configName: 'workstation', transfers: [sshTransfer(cleanRemote: false, excludes: '', execCommand: '', execTimeout: 120000, flatten: false, makeEmptyDirs: false, noDefaultExcludes: false, patternSeparator: '[, ]+', remoteDirectory: '/tmp', remoteDirectorySDF: false, removePrefix: '', sourceFiles: 'abc.txt')], usePromotionTimestamp: false, /useWorkspaceInPromotion: false, verbose: false)])
      }
    }
  }
}