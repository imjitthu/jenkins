pipeline {

  agent any

  options {
    copyArtifactPermission 'test-pipeline'
    durabilityHint 'MAX_SURVIVABILITY'
    //timestamps
  }
  stages {

    stage('frontend') {
      steps {
        cleanWs()
        powershell 'dir'
        archiveArtifacts artifacts: '*.*', followSymlinks: false, onlyIfSuccessful: true
        powershell 'pwd; ls'
      }
    }
  }
}